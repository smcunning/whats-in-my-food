<h1><%= @total_foods %> Foods</h1>
<% @foods.each do |food| %>
  <ul class="food">
    <li class="gtin-upc"><%= food.code %></li>
    <li class="description"><%= food.description %></li>
    <li class="brand-owner"><%= food.brand %></li>
    <li class="ingredients"><%= food.ingredients %></li>
  </ul>
<% end %>
